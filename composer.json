{
  "name": "swichers/acsf-client",
  "type": "library",
  "description": "A Client library for Acquia's ACSF platform.",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Steven Wichers",
      "email": "me@stevenwichers.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "doctrine/annotations": "^1.7",
    "symfony/config": "^4.3",
    "symfony/console": "^4.3",
    "symfony/dependency-injection": "^4.3",
    "symfony/finder": "^4.3",
    "symfony/http-client": "^4.3",
    "symfony/yaml": "^4.3"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
    "localheinz/composer-normalize": "^1.3",
    "phpunit/phpunit": "^6.5 || ^4.8",
    "drupal/coder": "^8.3"
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "swichers\\Acsf\\Client\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "swichers\\Acsf\\Client\\Tests\\": "tests/Unit"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "phpcs": "vendor/bin/phpcs"
  }
}
